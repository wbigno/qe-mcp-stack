        {
          "method": "POST",
          "path": "/api/users",
          "description": "Create new user",
          "body": {
            "email": "string (required)",
            "name": "string (required)",
            "role": "string (optional)"
          },
          "responses": [
            {"status": 201, "description": "Created - returns new user"},
            {"status": 400, "description": "Bad request - validation error"},
