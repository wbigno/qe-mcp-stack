{
  "name": "qe-mcp-orchestrator",
  "version": "1.0.0",
  "description": "Central orchestrator for QE MCP testing stack",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:report": "npm test && open test-reports/orchestrator-test-report.html",
    "test:unit": "npm test -- tests/unit",
    "test:integration": "npm test -- tests/integration",
    "lint": "eslint src/"
  },
  "keywords": [
    "mcp",
    "testing",
    "automation",
    "dotnet",
    "azure-devops"
  ],
  "author": "Your Company",
  "license": "UNLICENSED",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "axios": "^1.6.2",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
